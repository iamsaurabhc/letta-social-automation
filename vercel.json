{
    "version": 2,
    "buildCommand": "node ../../setup-env.js && pnpm build",
    "installCommand": "pnpm install",
    "builds": [
      {
        "src": "apps/client/next.config.js",
        "use": "@vercel/next",
        "config": {
          "outputDirectory": "apps/client/.next"
        }
      },
      {
        "src": "apps/server/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/apps/server/src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/apps/client/$1"
      }
    ]
  }