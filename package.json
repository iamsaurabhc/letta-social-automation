{
    "name": "monorepo-template",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "setup:env": "node setup-env.js",
        "dev": "pnpm setup:env && pnpm build:common && pnpm --parallel --stream -r dev",
        "build:common": "pnpm --filter common build",
        "build": "pnpm build:common && pnpm --filter client build && pnpm --filter server build",
        "vercel-build": "pnpm build && pnpm --filter server vercel-build",
        "start": "pnpm --parallel -r start",
        "clean": "rm -rf node_modules **/node_modules",
        "install:clean": "pnpm clean && pnpm install",
        "test": "pnpm -r test",
        "lint": "pnpm -r lint"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.x",
    "dependencies": {
        "@nestjs/bull": "^11.0.2",
        "@nestjs/common": "^11.0.10",
        "@nestjs/config": "^4.0.0",
        "@nestjs/platform-express": "^11.0.10",
        "@types/cors": "^2.8.17",
        "axios": "^1.7.9",
        "bull": "^4.16.5",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.4"
    }
}
